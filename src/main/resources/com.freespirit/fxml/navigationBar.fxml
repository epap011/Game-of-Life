<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>

<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:id="navBar" fx:controller="com.freespirit.controllers.NavigationBarController"  >

    <Button fx:id="startButton" text="START" > </Button>
    <Button fx:id="nextButton"  text="NEXT" > </Button>
    <Button fx:id="pauseButton" text="PAUSE" > </Button>

    <Region HBox.hgrow="ALWAYS" />

    <Slider fx:id="speedSlider" min="10" max="1500" value="300" majorTickUnit="10" > </Slider>
</HBox>
